{
  "name": "@quenk/potoo",
  "version": "0.3.2",
  "description": "An actor model library for JavaScript.",
  "main": "lib/index.js",
  "types":"./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "./node_modules/.bin/mocha --opts mocha.opts test/func",
    "clean": "rm -R ./lib/*",
    "examples": "npm run -s build:examples && ./node_modules/.bin/browserify --debug -o examples/examples.js examples/index.js",
    "build:lib": "rm -R lib/*; tsc --project src/tsconfig.json",
    "build:examples": "tsc examples/index.ts",
    "prep:test": "npm run build:lib; rm -R ./node_modules/potoo; mkdir -p node_modules/potoo; cp -R lib node_modules/potoo; cp package.json node_modules/potoo/package.json"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.40",
    "fluture": "^5.0.0",
    "mocha": "^3.2.0",
    "must": "^0.13.4",
    "source-map-support": "^0.4.3",
    "ts-node": "^3.0.2",
    "babel-cli": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "browserify": "^14.1.0"
  },
  "author": "Quenk Technologies",
  "license": "Apache-2.0",
  "dependencies": {
    "afpl": "0.0.1",
    "bluebird": "^3.5.0"
  }
}
